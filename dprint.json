{
  "plugins": [
    "https://plugins.dprint.dev/prettier-0.58.2.json@2fd8cc9c9773364848dc93735b01a00d55fcfa78a56a9230b4b63c014964c82d"
  ],
  "excludes": ["**/node_modules", "**/*.d.ts", "**/CHANGELOG.md"],
  "includes": ["**/*.ts", "**/*.md", "**/*.json"],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "quoteProps": "as-needed",
    "insertPragma": false,
    "requirePragma": false,
    "proseWrap": "preserve"
  }
}
